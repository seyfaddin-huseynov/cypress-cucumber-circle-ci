{
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^15.1.3",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "cypress": "^12.5.1",
    "rxjs": "^6.6.3",
    "typescript": "^4.7.3"
  },
  "cypress-cucumber-preprocessor": {
    "omitFiltered": true,
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": [
      "cypress/e2e/[filepath]/**/*.{js,ts}",
      "cypress/e2e/[filepath].{js,ts}",
      "cypress/e2e/common/**/*.{js,ts}"
    ]
  }
}
